# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# S3 Configuration
AWS_BUCKET_NAME=photography-portfolio-images
CLOUDFRONT_URL=https://your-cloudfront-url.cloudfront.net

# DynamoDB Tables
DYNAMODB_GALLERIES_TABLE=galleries
DYNAMODB_IMAGES_TABLE=images
DYNAMODB_AUTH_SESSIONS_TABLE=auth-sessions

# SES Configuration
SES_REGION=us-east-1
SES_SENDER_EMAIL=noreply@yourdomain.com

# Admin Email (for authentication)
ADMIN_EMAIL=your@email.com

# Session Secret (generate a random string)
SESSION_SECRET=your_secure_random_secret_key_at_least_32_characters
